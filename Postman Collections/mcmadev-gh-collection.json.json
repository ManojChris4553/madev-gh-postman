          "const context = pm.environment.name ? pm.environment : pm.collectionVariables;",
          "",
          "const tokenRefreshTime = context.get(\"dne_tokenRefreshTime\") || 0;",
          "console.log(\"token refresh time: \" + tokenRefreshTime);",
